{
  "version": 2,
  "functions": {
    "api/protect.js": {
      "maxDuration": 10
    },
    "api/resolve.js": {
      "maxDuration": 10
    },
    "api/analytics.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/protect",
      "dest": "/api/protect.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/resolve",
      "dest": "/api/resolve.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/analytics",
      "dest": "/api/analytics.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}